
<%= form_for [@concert], :html => { :multipart => true } do |f|%>
	<%= f.label :name%>
	<%= f.text_field :name%>
	<br>
	<%= f.label :band%>
	<%= f.text_field :band%>
	<br>
	<%= f.label :venue%>
	<%= f.text_field :venue%>
	<br>
	<%= f.label :city%>
	<%= f.text_field :city%>
	<br>
	<%= f.label :date%>
	<%= f.date_field :date%>
	<br>
	<%= f.label :price%>
	<%= f.text_field :price%>
	<br>
	<%= f.label :description%>
	<%= f.text_field :description%>
	<br>
	<%= f.label :logo %>
	<%= f.file_field :logo %>
	<br>
	<%= f.submit 'Save'%>
<%end%>	